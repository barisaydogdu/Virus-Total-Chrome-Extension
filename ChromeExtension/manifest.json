{
  "manifest_version": 3,
  "name": "URL Analyzer",
  "version": "2.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "scripting",
    "notifications"

  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/logo16.png",
      "48": "icons/logo48.png",
      "128": "icons/logo128.png"
    }
  },
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  }
}
